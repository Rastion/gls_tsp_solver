{
    "entry_point": "gls_tsp_solver:GLSTSPSolver",
    "default_params": {
        "time_limit": 300,
        "lambda_param": 0.3,
        "a": 1,
        "k": 10
    },
    "optimizer_type": "Guided Local Search TSP Solver",
    "description": "TSP solver using Guided Local Search (GLS) on top of a 2-opt local search. Penalties on edges (features) are updated to escape local minima.",
    "compatible_problems": [
        "Traveling Salesman Problem"
    ],
    "parameters": {
        "time_limit": {
            "type": "int",
            "description": "Maximum allowed time in seconds."
        },
        "lambda_param": {
            "type": "float",
            "description": "Penalty multiplier parameter lambda."
        },
        "a": {
            "type": "float",
            "description": "Adaptive parameter initialization."
        },
        "k": {
            "type": "int",
            "description": "Number of features (example in TSP number of neighbors to keep a list for)."
        }
    },
    "requirements": [
        "qubots"
    ],
    "keywords": [
        "guided local search",
        "GLS",
        "TSP",
        "local search",
        "2-opt"
    ]
}
